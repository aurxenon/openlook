# XView.rules for XView release 3.0.  Imake rules needed by distribution.

SYS_LIBRARIES = -lrpcsvc
DEPLIBS =
LOCAL_LIBRARIES = $(XVIEWLIB) $(OLGXLIB) $(XLIB) $(XVCLIENTMISCLIB)

MAKEFILE_SIMPLE = Makefile.simple
ICON            = icon.xbm
SRCS            = perf.c
ALLFILES        = $(MAKEFILE_SIMPLE) $(SRCS) $(ICON)

PROGRAMS	= perf

all:: $(PROGRAMS)

perf: perf.o
	$(RM) $@
	$(CC) -o $@ perf.o $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS)   $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) perf

# @(#)XView.prog	1.6 28 Jun 1993 SMI

populate::
	@case '${MFLAGS}' in *[i]*) set +e;; esac;
	-@if [ -h SCCS ]; then \
	for i in $(ALLFILES) ; do sccs get $$i ; done ; \
	elif [ -h SCCS- ]; then \
	$(MV) SCCS- SCCS ; \
	for i in $(ALLFILES) ; do sccs get $$i ; done ; \
	$(MV) SCCS SCCS- ; \
	else \
	echo "SCCS or SCCS- do not exist" ; \
	fi

install.srcs::

includes::

install.man::

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

